    <appSettings>
      <!-- How long to display the match list in the team's admin section after the tournament has ended -->
      <add key="CurrentTournamentEndMatchUpdateDays" value="7"/>
      <!-- # of days to allow the teams to enter match scores after the match date -->
      <add key="DaysToAllowMatchScoreUpdatesAfterMatchDate" value="7"/>
      <!-- Add comma seperated match id's to allow them for updates by the respective teams -->
      <add key="MatchIDsForScoreUpdateAllowedCommaSeperated" value=""/>
      <!--  specify the student team names so they will get discount during the regsitration process of $60. this will appear on the paypal page -->
      <add key="StudentTeamNamesForFees" value="SMU Mustangs Blue,UTDCC"/>
      <!-- Set this to 1 if we want to display only weekends on tournament schedule edit page for the admin -->
      <add key="DisplayWeekendsOnlyforScheduleEntry" value="0"/>
      <!-- Set this to 1 if we want to display only weekends on tournament schedule edit page for the admin -->
      <add key="DisplayPlayerGuardianDataOnPlayerProfile" value="1"/>

      <add key="BonusPointsRuleSet1" value="Team scored >= 60 runs, Team restricted opponent to less than 60 runs, Batsman scored 21 or more runs, Bowler took more than 3 wickets in an over"/>
      <add key="BonusPointsRuleSet2" value="Team scored >= 60 runs, Team restricted opponent to less than 60 runs, Batsman scored 21 or more runs, Bowler took more than 3 wickets in an over"/>
      <add key="BonusPointsRuleSet3" value="Team scored >= 50 runs, Team restricted opponent to less than 50 runs, Batsman scored 15 or more runs, Bowler took more than 2 wickets in an over"/>
      <add key="BonusPointsRuleSet1TournamentID" value="6"/>
      <add key="BonusPointsRuleSet2TournamentID" value="5"/>
      <add key="BonusPointsRuleSet3TournamentID" value="4"/>

      <add key="AnnouncementsDisplayCount" value="30"/>

    </appSettings>

