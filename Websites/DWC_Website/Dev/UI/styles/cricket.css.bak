}

/* read only input fields */
.viewData
{
	background-color: #a5d69c;
	color: #000000;
	/* color: #0066CC; */
}

/* server side edit errors posted back */
.serverError
{
	font-family: helvetica;
	font-size: 10pt;
	color: #ff0000;
	text-align: left;
	border: 3 groove #efefef;
}

.serverErrorHeading
{
	font-size: 10pt;
	font-weight: bold;
}

